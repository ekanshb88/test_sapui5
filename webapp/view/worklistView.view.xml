<mvc:View controllerName="testproject.controller.worklistView"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <Text id="myTitleText" text="Hello World" />
            <Button id="idPressMeButton" text="Click Me" press=".onPress"/>
            <Text id="myText" text="Hello, SAP UI5!" />
            <Button id="idWarningButton" text="Do NOT Click Me" press=".onDoNotPress"/>
        </content>
    </Page>
</mvc:View>
